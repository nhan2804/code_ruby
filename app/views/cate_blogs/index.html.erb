<p id="notice"><%= notice %></p>

<h1>Cate Blogs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cate_blogs.each do |cate_blog| %>
      <tr>
        <td><%= link_to 'Show', cate_blog %></td>
        <td><%= link_to 'Edit', edit_cate_blog_path(cate_blog) %></td>
        <td><%= link_to 'Destroy', cate_blog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cate Blog', new_cate_blog_path %>
