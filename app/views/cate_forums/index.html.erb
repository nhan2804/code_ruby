<p id="notice"><%= notice %></p>

<h1>Cate Forums</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cate_forums.each do |cate_forum| %>
      <tr>
        <td><%= link_to 'Show', cate_forum %></td>
        <td><%= link_to 'Edit', edit_cate_forum_path(cate_forum) %></td>
        <td><%= link_to 'Destroy', cate_forum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cate Forum', new_cate_forum_path %>
