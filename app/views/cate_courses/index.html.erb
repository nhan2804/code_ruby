<p id="notice"><%= notice %></p>

<h1>Cate Courses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cate_courses.each do |cate_course| %>
      <tr>
        <td><%= link_to 'Show', cate_course %></td>
        <td><%= link_to 'Edit', edit_cate_course_path(cate_course) %></td>
        <td><%= link_to 'Destroy', cate_course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cate Course', new_cate_course_path %>
